<p id="notice"><%= notice %></p>

<h1>Listing Quote Details</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quote_details.each do |quote_detail| %>
      <tr>
        <td><%= link_to 'Show', quote_detail %></td>
        <td><%= link_to 'Edit', edit_quote_detail_path(quote_detail) %></td>
        <td><%= link_to 'Destroy', quote_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Quote detail', new_quote_detail_path %>
