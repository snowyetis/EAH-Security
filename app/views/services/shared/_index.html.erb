<section class="main-section paddind" id="Portfolio">
  <!--main-section-start-->
  <div class="container">
    <h2>Products</h2>
    <h6>Browse through some of the products we offer</h6>
    <div class="portfolioFilter">
      <ul class="Portfolio-nav wow fadeIn delay-02s">
        <li><a href="#" data-filter="*" class="current">All</a></li>
        <li><a href="#" data-filter=".branding">Security Solutions</a></li>
        <li><a href="#" data-filter=".webdesign">Locksmithing</a></li>
        <li><a href="#" data-filter=".printdesign">Safes</a></li>
        <li><a href="#" data-filter=".photography">Firedoor Safety</a></li>
      </ul>
    </div>

  </div>
  <div class="portfolioContainer wow fadeInUp delay-04s">
    <div class=" Portfolio-box printdesign">
      <%= link_to "#" do %>
        <%= image_tag "Portfolio-pic1.jpg" %>
      <% end %>
      <h3>Foto Album</h3>
      <p>Print Design</p>
    </div>
    <div class="Portfolio-box webdesign">
      <%= link_to "#" do %>
        <%= image_tag "Portfolio-pic2.jpg" %>
      <% end %>
      <h3>Luca Theme</h3>
      <p>Web Design</p>
    </div>
    <div class=" Portfolio-box branding">
      <%= link_to "#" do %>
        <%= image_tag "Portfolio-pic3.jpg" %>
      <% end %>
      <h3>Uni Sans</h3>
      <p>Branding</p>
    </div>
    <div class=" Portfolio-box photography">
      <%= link_to "#" do %>
        <%= image_tag "Portfolio-pic4.jpg" %>
      <% end %>
      <h3>Vinyl Record</h3>
      <p>Photography</p>
    </div>
    <div class=" Portfolio-box branding">
      <%= link_to "#" do %>
        <%= image_tag "Portfolio-pic5.jpg" %>
      <% end %>
      <h3>Hipster</h3>
      <p>Branding</p>
    </div>
    <div class=" Portfolio-box photography">
      <%= link_to "#" do %>
        <%= image_tag "Portfolio-pic6.jpg" %>
      <% end %>
      <h3>Windmills</h3>
      <p>Photography</p>
    </div>
  </div>
</section>
